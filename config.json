{
    "cardiology_protocols": {
        "indexing": {
            "name": "cardiology_protocols",
            "description": "Documents containing medical protocols for cardiologic patients.",
            "type": "faiss",
            "distance": "cosine",
            "alias": "qdrant_cardio_current"
        },
        "embeddings": {
            "deployment": "sentence-transformers/all-MiniLM-L6-v2",
            "kwargs": {}
        },
        "preprocessing": {
            "storage": {
                "parent_folder": "test_data",
                "input_folder": "pdfdocs",
                "output_folder": "mddocs",
                "allowed_extensions": [
                    "pdf"
                ]
            },
            "images": {
                "dpi": 200,
                "tol": 40.0,
                "pad": 16.0,
                "caption_keywords": [
                    "Figure"
                ]
            },
            "chunking": {
                "markdown_first": true,
                "header_levels": 4,
                "splitter": "recursive",
                "chunk_size": 1000,
                "chunk_overlap": 200
            }
        }
    }
}